  <DataSets>
    <DataSet Name="Consulta2">
      <Query>
        <DataSourceName>Agencia</DataSourceName>
        <CommandText>SELECT *
FROM (SELECT COUNT(curp) AS num_duenios
FROM APP.AGENCIA.DUENIOS) AS d, (SELECT COUNT(curp) AS num_choferes
FROM APP.AGENCIA.CHOFERES) AS ch;</CommandText>
      </Query>
      <Fields>
        <Field Name="num_duenios">
          <DataField>num_duenios</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="num_choferes">
          <DataField>num_choferes</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Consulta3">
      <Query>
        <DataSourceName>Agencia</DataSourceName>
        <CommandText>SELECT DATEPART(M, i.fecha) AS mes_ingreso, COUNT(DATEPART(M, i.fecha)) AS num_choferes
FROM APP.AGENCIA.CHOFERES ch INNER JOIN APP.AGENCIA.INGRESAR i ON ch.curp = i.curp
WHERE DATEPART(YYYY, i.fecha) = DATEPART(YYYY, GETDATE())
GROUP BY DATEPART(MONTH, i.fecha);
</CommandText>
      </Query>
      <Fields>
        <Field Name="mes_ingreso">
          <DataField>mes_ingreso</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="num_choferes">
          <DataField>num_choferes</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
